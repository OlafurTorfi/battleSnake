{
  "name": "battlesnake-node",
  "description": "A simple BattleSnake AI written in Javascript for NodeJS",
  "version": "0.0.0",
  "private": true,
  "author": "Sendwithus",
  "repository": "https://github.com/sendwithus/battlesnake-node",
  "licenses": [
    {
      "type": "WTFPL",
      "url": "http://www.wtfpl.net/txt/copying/"
    }
  ],
  "dependencies": {
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.9.5",
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "mocha": "^6.0.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  },
  "main": "./lib/index.js",
  "engines": {
    "node": ">=8.x"
  },
  "scripts": {
    "start": "cross-env PORT=5000 nodemon lib/index.js",
    "build:commonjs": "rimraf lib && tsc --target es5 --outDir lib --module commonjs --declaration",
    "build:es": "rimraf es && tsc --target es6 --outDir es --module es6",
    "build": "npm run build:commonjs && npm run build:es",
    "ts": "ts-node src/",
    "prepublishOnly": "npm run build",
    "test": "mocha ./lib/**/*.spec.js",
    "tdd": "mocha -R dot --require ts-node/register --recursive --watch-extensions ts \"./ts/**/*.spec.ts\" --watch --color"
  },
  "files": [
    "es",
    "lib"
  ],
  "types": "./lib/index.d.ts",
  "jsnext:main": "./es/index.js",
  "module": "./es/index.js"
}
